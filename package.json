{
  "name": "@slothkit/eslint-config",
  "version": "0.0.2",
  "description": "eslint shareable configs",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./typescript": "./typescript.js",
    "./rules/react": "./rules/react.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slothkit/eslint-config.git"
  },
  "keywords": [
    "slothkit",
    "eslint",
    "airbnb"
  ],
  "author": "MichaelTZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slothkit/eslint-config/issues"
  },
  "homepage": "https://github.com/slothkit/eslint-config#readme",
  "scripts": {
    "commit": "git add . && cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "eslint --report-unused-disable-directives ."
  },
  "peerDependencies": {
    "eslint": "^8.2.0",
    "typescript": "*"
  },
  "devDependencies": {
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.37.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
